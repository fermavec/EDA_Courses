ro para trabajar con menos paises**"],"metadata":{"id":"m_xBpWJ8WsrJ"}},{"cell_type":"code","source":["idx_filter = df_pop_copy['Country'].isin(['Mexico', 'Canada'])\n","idx_filter"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"SW3mIPoVV2Mj","executionInfo":{"status":"ok","timestamp":1641421730717,"user_tz":360,"elapsed":135,"user":{"displayName":"Fer Mavec","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"06686205667122495301"}},"outputId":"daeffd2b-0468-46ef-ff5a-87b724014440"},"execution_count":37,"outputs":[{"output_type":"execute_result","data":{"text/plain":["0       False\n","1       False\n","2       False\n","3       False\n","4       False\n","        ...  \n","1035    False\n","1036    False\n","1037    False\n","1038    False\n","1039    False\n","Name: Country, Length: 1040, dtype: bool"]},"metadata":{},"execution_count":37}]},{"cell_type":"code","source":["df = df_pop_copy[idx_pop]\n","df"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":300},"id":"HVAvwpC-W_Ih","executionInfo":{"status":"ok","timestamp":1641421822549,"user_tz":360,"elapsed":122,"user":{"displayName":"Fer Mavec","photoUrl":"https://lh3.googleusercontent.com/a/default-user=s64","userId":"06686205667122495301"}},"outputId":"e1c49edc-48a6-4da0-940d-3b90e2a2bd8f"},"execution_count":38,"outputs":[{"output_type":"execute_result","data":{"text/html":["\n","  <div id=\"df-4d87bc0c-14ae-4f57-a77f-3c779547200d\">\n","    <div class=\"colab-df-container\">\n","      <div>\n","<style scoped>\n","    .dataframe tbody tr th:only-of-type {\n","        vertical-align: middle;\n","    }\n","\n","    .dataframe tbody tr th {\n","        vertical-align: top;\n","    }\n","\n","    .dataframe thead th {\n","        text-align: right;\n","    }\n","</style>\n","<table border=\"1\" class=\"dataframe\">\n","  <thead>\n","    <tr style=\"text-align: right;\">\n","      <th></th>\n","      <th>Country</th>\n","      <th>year</th>\n","      <th>p